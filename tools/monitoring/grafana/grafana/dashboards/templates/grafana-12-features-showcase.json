          "expr": "100 - ((node_filesystem_avail_bytes{fstype!~\"tmpfs|fuse.*\",mountpoint!~\"/boot.*\"} * 100) / node_filesystem_size_bytes{fstype!~\"tmpfs|fuse.*\",mountpoint!~\"/boot.*\"})",
